---
import Main from "@/components/home/main";
import SEO from "@/components/seo/index.astro";
import Schema from "@/components/structured-data/index.astro";
import RootLayout from "@/layouts/RootLayout.astro";

const aboutColor = "#7c3aed"; // violet-600
const contactColor = "#6d28d9"; // violet-700
---

<RootLayout>
  <SEO slot="seo" />
  <Schema slot="ld+json" type="WebSite" />
  <Main client:only="react" aboutColor={aboutColor} contactColor={contactColor} />
</RootLayout>
